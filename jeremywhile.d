import std.stdio; // for writeln function

void main() { // start of program
	int x = 0; // create an number object called x and set its value to 0

	while(x < 10) // while the value of x is less than ten do the loop
	{
		writeln(x); // print the value of x to the console
		x++; // increase the value of x by one
	}
}
